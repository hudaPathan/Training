<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomButtonStyle" parent="Theme.AppCompat">
        <item name="android:background">@drawable/btn</item>
        <item name="android:textColor">#000000</item>
        <item name="android:padding">12dp</item>
        <item name="android:layout_margin">15dp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="CustomButtonStyle2" >
        <item name="android:background">@color/secondary</item>



    </style>

    <style name="CheckBox">

        <item name="checkboxStyle">@drawable/checkbox_state</item>
    </style>

    <style name="tabTextAppearance">
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
    </style>

    <style name="RoundCorner">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

</resources>